  <!-- Seldore Body Gene -->

  <GeneDef>
    <defName>Gene_SeldoreBody</defName>
    <label>Seldore body</label>
    <description>Carriers of this gene will have a muscular body.</description>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
    <canGenerateInGeneSet>false</canGenerateInGeneSet>
    <displayCategory>CraveSaga</displayCategory>
    <displayOrderInCategory>401</displayOrderInCategory>
    <iconPath>Humanlike/Bodies/SeldoreBody/SeldoreBody_south</iconPath>
    <bodyType>Hulk</bodyType>
    <exclusionTags>
      <li>Fur</li>
    </exclusionTags>
    <fur>SeldoreBody</fur>
    <renderNodeProperties>
      <li>
        <debugLabel>Fur</debugLabel>
        <nodeClass>PawnRenderNode_Fur</nodeClass>
        <workerClass>PawnRenderNodeWorker_Fur</workerClass>
        <useSkinShader>false</useSkinShader>
        <baseLayer>5</baseLayer>
        <rotDrawMode>Fresh, Rotting</rotDrawMode>
        <colorType>Hair</colorType>
        <parentTagDef>Body</parentTagDef>
      </li>
    </renderNodeProperties>
    <modExtensions>
      <li Class="VEF.Genes.GeneExtension">
        <furHidesBody>true</furHidesBody>
        <dontColourFur>true</dontColourFur>
      </li>
    </modExtensions>
  </GeneDef>

